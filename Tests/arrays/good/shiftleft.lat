void shiftLeft (int [] a){
  int x = a [0];
  int i = 0 ;
  while (i < a.length - 1){
    a [i] = a [i + 1];
    i ++ ;
  }
  a[a.length - 1]= x ;
  return;
 }

int main () {
  int [] a = new int [5];
  int i = 0 ;
  while (i < a.length){
    a [i]= i ;
    i ++ ;
    }
  shiftLeft (a);
  for(int x : a)
  {
    printInt(x);
  }

  return 0 ;
}
